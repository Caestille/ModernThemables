<Application
	x:Class="ThemableWindow.App"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	<Application.Resources>
		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
				<ResourceDictionary Source="Themes/Icons.xaml" />
				<ResourceDictionary Source="Themes/Generic.xaml" />
				<ResourceDictionary Source="Themes/Rounded.xaml" />
				<ResourceDictionary Source="Themes/Material.xaml" />
				<ResourceDictionary Source="Themes/Windows10.xaml" />
				<ResourceDictionary Source="/CoreUtilities;component/Themes/Converters.xaml" />
			</ResourceDictionary.MergedDictionaries>
		</ResourceDictionary>
	</Application.Resources>
</Application>
