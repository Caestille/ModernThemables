                            BorderThickness="{TemplateBinding StrokeThickness,
                                                              ConverterParameter=1-1-1-1,
                                                              Converter={StaticResource DoubleToBorderThicknessConverter}}" />
                        <Grid
                            x:Name="Grid2"
                            Grid.ColumnSpan="5"
                            Margin="{TemplateBinding StrokeThickness,
                                                     ConverterParameter=1-1-1-1,
                                                     Converter={StaticResource DoubleToBorderThicknessConverter}}">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="0.2*" />
                                <RowDefinition Height="0.2*" />
                                <RowDefinition Height="0.2*" />
                                <RowDefinition Height="0.2*" />
                                <RowDefinition Height="0.2*" />
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="{Binding ElementName=Grid2, Path=ActualHeight}" />
                                <ColumnDefinition Width="0.15*" />
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="0.15*" />
                            </Grid.ColumnDefinitions>
                            <Grid Grid.RowSpan="5" Margin="{TemplateBinding StrokeThickness, Converter={StaticResource DoubleToBorderThicknessConverter}, ConverterParameter=2-2-2-2}">

                                <Grid.RowDefinitions>
                                    <RowDefinition Height="0.5*" />
                                    <RowDefinition Height="0.5*" />
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="0.5*" />
                                    <ColumnDefinition Width="0.5*" />
                                </Grid.ColumnDefinitions>
                                <Ellipse
                                    Grid.RowSpan="2"
                                    Grid.ColumnSpan="2"
                                    Fill="{TemplateBinding Foreground}" />
                                <Border
                                    Grid.Column="1"
                                    Width="{TemplateBinding StrokeThickness}"
                                    HorizontalAlignment="Left"
                                    Background="{TemplateBinding Background}" />
                                <Border
                                    Grid.Row="1"
                                    Grid.Column="1"
                                    Width="{TemplateBinding StrokeThickness}"
                                    HorizontalAlignment="Left"
                                    Background="{TemplateBinding Background}"
                                    RenderTransformOrigin="0,0">
                                    <Border.RenderTransform>
                                        <RotateTransform Angle="-45" />
                                    </Border.RenderTransform>
                                </Border>
                            </Grid>
                            <Border
                                Grid.Row="1"
                                Grid.Column="2"
                                Background="{TemplateBinding Foreground}" />
                            <Border
                                Grid.Row="3"
                                Grid.Column="2"
                                Background="{TemplateBinding Foreground}" />
                        </Grid>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
